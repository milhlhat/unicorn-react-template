{
  "name": "yody-ui",
  "version": "0.0.1",
  "description": "UI components for Yody system and for everyone",
  "scripts": {
    "start": "env-cmd -f .env.development webpack serve --config config/webpack.dev.js",
    "build": "env-cmd -f .env.production webpack --config config/webpack.prod.js",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" -c .eslintrc.json",
    "lint:fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\" -c .eslintrc.json",
    "format": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "format:fix": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,scss,md}\""
  },
  "repository": {
    "type": "git",
    "url": "latter"
  },
  "author": "YODY",
  "dependencies": {
    "antd": "^4.21.5",
    "fs": "^0.0.1-security",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "yody-ui": "^0.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-react": "^7.18.6",
    "@svgr/webpack": "^6.2.1",
    "@types/lodash": "4.14.181",
    "@types/node": "16.11.26",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "autoprefixer": "10.4.4",
    "babel-loader": "^8.2.5",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "10.2.4",
    "core-js": "3.21.1",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "3.4.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "folder-hash": "4.0.2",
    "fork-ts-checker-webpack-plugin": "7.2.3",
    "html-webpack-plugin": "5.5.0",
    "husky": "4",
    "identity-obj-proxy": "3.0.0",
    "json-loader": "0.5.7",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "merge-jsons-webpack-plugin": "2.0.1",
    "mini-css-extract-plugin": "2.6.0",
    "path-browserify": "^1.0.1",
    "postcss-loader": "6.2.1",
    "prettier": "^2.7.1",
    "rimraf": "3.0.2",
    "sass": "1.49.11",
    "sass-loader": "12.6.0",
    "simple-progress-webpack-plugin": "2.0.0",
    "sinon": "13.0.1",
    "source-map-loader": "3.0.1",
    "sourcemap-istanbul-instrumenter-loader": "0.2.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "thread-loader": "3.0.4",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "wait-on": "6.0.1",
    "webpack": "5.71.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "4.8.0",
    "webpack-merge": "5.8.0",
    "workbox-webpack-plugin": "6.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
